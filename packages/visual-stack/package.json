    "chai-enzyme": "^0.6.1",
    "css-loader": "^0.28.7",
    "enzyme": "^2.1.0",
    "eslint": "^2.2.0",
    "eslint-plugin-react": "^4.1.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.4",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-filter": "^4.0.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-plumber": "^1.1.0",
