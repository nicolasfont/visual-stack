{
  "name": "@cjdev/visual-stack",
  "version": "3.3.0",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "gulp build && npm run dist",
    "dist": "npm run dist:umd && npm run dist:min",
    "dist:umd": "NODE_ENV=dev webpack",
    "dist:min": "NODE_ENV=production webpack",
    "clean": "rimraf lib && rimraf dist",
    "lint": "eslint src test",
    "format": "prettier \"./src/**/*.{css,js}\" --write",
    "prepublish": "npm run clean && npm run lint && npm run build && npm run test",
    "test": "mocha --require @babel/register --require ignore-styles -r jsdom-global/register --recursive test",
    "test:watch": "npm run test -- --watch",
    "watch": "gulp build && gulp watch"
  },
  "peerDependencies": {
    "react": ">=15.3.1"
  },
  "dependencies": {
    "@material-ui/core": "^3.3.2",
    "@material-ui/icons": "^3.0.1",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "mdi-react": "^2.0.46",
    "normalize.css": "^8.0.1",
    "ramda": "^0.19.1",
    "react-responsive": "^1.3.0",
    "react-svg": "^2.1.19"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.5",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "css-loader": "^0.28.7",
    "enzyme": "^2.1.0",
    "eslint": "^2.2.0",
    "eslint-plugin-react": "^4.1.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.4",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-filter": "^4.0.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-plumber": "^1.2.1",
    "ignore-loader": "^0.1.2",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.9.1",
    "jsdom-global": "^2.1.0",
    "mocha": "^5.2.0",
    "natives": "^1.1.6",
