{
  "name": "@cjdev/visual-stack-redux",
  "version": "3.4.0",
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "build": "gulp build && npm run dist",
    "dist": "npm run dist:umd && npm run dist:min",
    "dist:umd": "NODE_ENV=dev webpack src/index.js dist/visual-stack-redux.js",
    "dist:min": "NODE_ENV=production webpack src/index.js dist/visual-stack-redux.min.js",
    "clean": "rimraf lib",
    "lint": "eslint src test",
    "format": "prettier \"./src/**/*.js\" --write",
    "prepublish": "npm run clean && npm run lint && npm run build && npm run test",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "watch": "gulp build && gulp watch"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/__mocks__/styleMock.js"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "peerDependencies": {
    "@cjdev/visual-stack": "^2.5.1",
    "react": ">=15.3.1",
    "react-dom": ">=15.3.1",
    "redux": ">=3.3.1"
  },
  "dependencies": {
    "ramda": "^0.19.1",
    "redux-actions": "^0.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@cjdev/visual-stack": "^3.4.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.8.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-15": "^1.3.0",
    "eslint": "^5.13.0",
    "eslint-plugin-react": "^7.12.4",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-plumber": "^1.1.0",
    "ignore-loader": "^0.1.2",
    "ignore-styles": "^5.0.1",
    "jest": "^24.1.0",
    "jsdom": "^13.2.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "natives": "^1.1.6",
    "prettier": "^1.7.0",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^4.3.11",
    "react-redux": "^4.4.1",
    "react-test-renderer": "^15.5.0",
    "redux": "^3.3.1",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "webpack": "^3.6.0"
  }
}
