{
  "name": "@cjdev/visual-stack-docs",
  "version": "1.1.0",
  "private": true,
  "devDependencies": {
    "concurrently": "^3.1.0",
    "copyfiles": "^1.2.0",
    "eslint": "^3.14.1",
    "npm-watch": "^0.1.8",
    "react-scripts": "^0.9.5"
  },
  "dependencies": {
    "@cjdev/visual-stack": "^1.1.0",
    "@cjdev/visual-stack-redux": "^1.1.0",
    "express": "^4.14.1",
    "material-ui": "^1.0.0-beta.8",
    "material-ui-icons": "^1.0.0-beta.5",
    "mdi-react": "^2.0.46",
    "morgan": "^1.8.0",
    "ramda": "^0.23.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.2",
    "react-responsive": "^1.3.0",
    "react-router": "^3.0.4",
    "redux": "^3.6.0"
  },
  "proxy": "http://localhost:3001/",
  "watch": {
    "samples": "src/containers/Components/Docs/*.js"
  },
  "scripts": {
    "samples": "copyfiles -f src/containers/Components/Docs/*.js build/samples/",
    "start": "react-scripts start",
    "build": "npm run samples && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "server": "concurrently npm-watch \"node ./server\"",
    "docs": "concurrently \"npm run start\" \"npm run server\""
  }
}
